{
    "manifest_version": 2,
    "name": "Social Media Blocker",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": { "48": "48.png", "128": "128.png"},               
    "permissions": [
        "unlimitedStorage",
        "notifications",
        "https://www.youtube.com/*",
        "https://www.facebook.com/*",
        "https://www.instagram.com/",
        "discord.com"
    ]
}